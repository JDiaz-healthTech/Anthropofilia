{
    "name": "anthropofilia/blog",
    "description": "Blog personal con panel de administraciÃ³n",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ezyang/htmlpurifier": "^4.19",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "@composer dump-autoload -o"
        ],
        "post-update-cmd": [
            "@composer dump-autoload -o"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
